{
    "-allow-seed-extension": "true",
    "-allow-seed-truncation": "true",
    "-disable-inlining": "true",
    "-only-output-states-covering-new": "true",
    "-optimize": "true",
    "-output-module": "true",
    "-posix-runtime": "true",
    "-simplify-sym-indices": "true",
    "-use-batching-search": "true",
    "-use-cex-cache": "true",
    "-use-forked-solver": "true",
    "-watchdog": "true",
    "-write-cvcs": "true",
    "-write-cov": "true",
    "-max-memory-inhibit": "false",

    "-batch-instructions": 10000,
    "-max-memory": 1000,
    "-max-sym-array-size": 4096,

    "-max-static-cpfork-pct": 1.0,
    "-max-static-fork-pct": 1.0,
    "-max-static-solve-pct": 1.0,

    "-max-solver-time": "30s",
    
    "-external-calls": "all",
    "-libc": "uclibc",
    "-search": "random-path -search=nurs:covnew",
    "-switch-type": "internal",
    
    "-sym-stdout": "on",
    "-sym-args": "0 1 10 -sym-args 0 2 2",
    "-sym-files": "1 8",
    "-sym-stdin": 8
}

